{
  "private": true,
  "name": "warpgate",
  "description": "A lightweight browser extension to jump to various external bookmarks via the address bar.",
  "version": "1.1.0",
  "license": "MPL-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheap-glitch/warpgate.git"
  },
  "bugs": {
    "url": "https://github.com/cheap-glitch/warpgate/issues"
  },
  "author": "cheap glitch <cheap.glitch@gmail.com> (https://github.com/cheap-glitch)",
  "homepage": "https://github.com/cheap-glitch/warpgate#readme",
  "scripts": {
    "build": "node build.js",
    "lint": "run-s 'lint:*'",
    "lint:js": "eslint '**/*.js'",
    "lint:webext": "web-ext lint",
    "postinstall": "cp node_modules/webextension-polyfill/dist/browser-polyfill.min.js src/scripts/browser-polyfill.js",
    "serve": "cross-env GTK_THEME=Numix:light web-ext run --firefox-profile dev --keep-profile-changes --url about:debugging#/runtime/this-firefox"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "cross-zip": "^4.0.0",
    "eslint": "^7.14.0",
    "eslint-plugin-no-unsanitized": "^3.1.4",
    "eslint-plugin-smarter-tabs": "^1.1.2",
    "fs-extra": "^9.0.1",
    "html-minifier": "^4.0.0",
    "klaw-sync": "^6.0.0",
    "mocha": "^8.2.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "terser": "^5.5.1",
    "web-ext": "^5.4.0",
    "webextension-polyfill": "^0.7.0"
  }
}
